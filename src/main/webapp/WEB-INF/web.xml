<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>primerservlet</servlet-name>
        <servlet-class>servlets.primerservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>letranif</servlet-name>
        <servlet-class>servlets.letranif</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>primerservlet</servlet-name>
        <url-pattern>/primerservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>letranif</servlet-name>
        <url-pattern>/letranif</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
